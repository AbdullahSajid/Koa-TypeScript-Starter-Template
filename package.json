{
  "name": "koa-typescript-starter-template",
  "version": "1.0.0",
  "description": "Koa.js Starter Template based on Typescript",
  "main": "src/index.ts",
  "scripts": {
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\"",
    "lint-and-fix": "tsc --noEmit && eslint . --ext .js,.ts --quiet --fix",
    "build": "rimraf dist && tsc",
    "test": "set DEBUG=* & nodemon src/index.ts",
    "dev": "nodemon src/index.ts",
    "serve": "npm run build && node dist/index.js"
  },
  "author": "Abdullah Sajid",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.6.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^5.0.1",
    "koa-helmet": "^6.1.0",
    "koa-morgan": "^1.0.1",
    "koa-req-validation": "^0.15.0",
    "mongoose": "^5.13.5",
    "mongoose-paginate-v2": "^1.4.1",
    "rate-limiter-flexible": "^2.2.4",
    "request-ip": "^2.1.3",
    "validator": "^13.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.2",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-morgan": "^1.0.5",
    "@types/koa__cors": "^3.0.3",
    "@types/koa__router": "^8.0.7",
    "@types/mongoose-paginate-v2": "^1.3.11",
    "@types/node": "^16.4.9",
    "@types/request-ip": "0.0.37",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
